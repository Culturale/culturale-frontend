    "@react-navigation/stack": "^6.3.16",
    "@testing-library/react-hooks": "^6.0.0",
    "@testing-library/react-native": "^12.0.1",
    "aws-sdk": "^2.1377.0",
    "axios": "^1.4.0",
    "expo": "^48.0.16",
