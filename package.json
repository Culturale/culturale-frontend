    "expo-location": "^15.1.1",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "i18n-js": "^3.8.0",
    "jest-environment-jsdom": "^29.5.0",
    "jwt-decode": "^3.1.2",
    "mobx": "^6.9.0",
