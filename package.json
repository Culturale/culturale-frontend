{
  "name": "culturale-front",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "startMetro": "react-native start --reset-cache",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "npx typesync",
    "androidNative": "react-native run-android",
    "iosNative": "react-native run-ios",
    "android:production:apk": "cd android && ./gradlew assembleRelease && ./gradlew installRelease && cd ..",
    "android:clean": "cd android/ && ./gradlew clean",
    "android:reverse": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build:android": "cd android && ./gradlew app:assembleRelease && cd ..",
    "build:ios": "xcodebuild -workspace ios/paackDrivers.xcworkspace -scheme paackDrivers -configuration Release -derivedDataPath ios/build -destination 'name=iPhone 11'",
    "lint": "eslint app test --ext .js,.ts,.tsx",
    "lint:no-cycles": "eslint app test --ext .js,.ts,.tsx --rule 'import/no-cycle: 2'",
    "startServer": "react-native start --reset-cache",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "tsc": "tsc --noEmit -p . --pretty",
    "fullClean": "rm -rf $HOME/.gradle/caches/ && yarn ac && cd .. rm -rf node_modules && yarn install",
    "install:pods": "cd ios && pod-install --quiet",
    "install:deps": "yarn install && yarn install:pods",
    "parseSchema": "npx openapi-typescript ./app/infrastructure/api/open-api.yml --output ./app/infrastructure/api/schema.ts"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
